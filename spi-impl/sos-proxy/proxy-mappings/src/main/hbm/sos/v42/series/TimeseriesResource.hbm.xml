<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.n52.series.db.beans.DatasetEntity" table="series">
    <id name="pkid" type="long">
      <column name="seriesid"/>
      <generator class="assigned"/>
    </id>
    <discriminator formula="&apos;measurement&apos;" type="string"/>
    <many-to-one class="org.n52.series.db.beans.CategoryEntity" column="observablepropertyid" insert="false" name="category" update="false"/>
    <many-to-one class="org.n52.series.db.beans.ProcedureEntity" column="procedureid" insert="false" name="procedure" update="false"/>
    <many-to-one class="org.n52.series.db.beans.PhenomenonEntity" column="observablepropertyid" insert="false" name="phenomenon" update="false"/>
    <many-to-one class="org.n52.series.db.beans.FeatureEntity" column="featureofinterestid" insert="false" name="feature" update="false"/>
    <many-to-one class="org.n52.series.db.beans.PlatformEntity" column="procedureid" name="platform"/>
    <subclass discriminator-value="measurement" name="org.n52.series.db.beans.ext.MeasurementSeriesEntity">
      <property formula="3" name="numberOfDecimals" type="int"/>
      <property column="published" name="published" type="org.hibernate.type.TrueFalseType"/>
      <many-to-one class="org.n52.series.db.beans.UnitEntity" column="seriesid" insert="false" name="unit" update="false"/>
      <component class="org.n52.series.db.beans.ext.MeasurementEntity" name="firstValue">
        <property column="firsttimestamp" name="timestamp" type="timestamp"/>
        <property column="firstnumericvalue" name="value" type="double"/>
      </component>
      <component class="org.n52.series.db.beans.ext.MeasurementEntity" name="lastValue">
        <property column="lasttimestamp" name="timestamp" type="timestamp"/>
        <property column="lastnumericvalue" name="value" type="double"/>
      </component>
      <list fetch="select" inverse="true" name="observations" table="observation">
        <key column="observationid"/>
        <index column="seriesid"/>
        <one-to-many class="org.n52.series.db.beans.ext.MeasurementEntity"/>
      </list>
    </subclass>
