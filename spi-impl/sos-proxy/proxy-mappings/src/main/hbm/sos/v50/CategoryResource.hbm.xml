<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.n52.series.db.beans">
    <class name="CategoryEntity" table="category">
        <id name="pkid" type="long">
            <column name="categoryid"/>
            <generator class="native">
                <param name="sequence">categoryId_seq</param>
            </generator>
        </id>
        <property column="identifier" name="domainId" type="string" not-null="true"/>
        <property column="name" name="name" type="string"/>
        <many-to-one class="ServiceEntity" column="serviceid" name="service"/>
    </class>
</hibernate-mapping>
